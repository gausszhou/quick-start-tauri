{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "GTools",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "app": {
        "all": false
      },
      "clipboard": {
        "all": false
      },
      "dialog": {
        "all": false
      },
      "fs": {
        "all": false
      },
      "globalShortcut": {
        "all": false
      },
      "http":{
        "all": false
      },
      "notification": {
        "all": false
      },
      "os": {
        "all": false
      },
      "path": {
        "all": false
      },
      "shell": {
        "all": false
      },
      "window":{
        "all": false
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.build",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "GTools",
        "center": true,
        "width": 1366,
        "height": 768
      }
    ]
  }
}
